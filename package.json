{
  "name": "mycn",
  "version": "0.12.0",
  "description": "A layer above SQL database connectors for Node.js, inspired from PDO and JDBC",
  "main": "mycn.min.js",
  "types": "mycn.d.ts",
  "devDependencies": {
    "rollup": "0.64.1",
    "typescript": "3.0.1",
    "uglify-es": "3.3.9",
    "npm-run-all": "4.1.3",
    "rimraf": "2.6.2"
  },
  "scripts": {
    "_clear": "rimraf build/compiled/*",
    "_tsc": "tsc",
    "_make-bundle": "node build/make-bundle",
    "build": "run-s _clear _tsc _make-bundle",
    "watch": "tsc --watch"
  },
  "engines": {
    "node": ">=8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paleo/mycn.git"
  },
  "author": {
    "name": "Paleo"
  },
  "license": "CC0-1.0",
  "keywords": [
    "connection",
    "connector",
    "relational",
    "database",
    "pool",
    "transaction",
    "sql",
    "sqlite",
    "postgresql",
    "jdbc",
    "pdo"
  ]
}
