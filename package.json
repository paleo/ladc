{
  "name": "ladc",
  "version": "0.17.1",
  "description": "A Layer Above Database Connectors, that provides a common API inspired from PDO and JDBC.",
  "main": "ladc.min.js",
  "types": "ladc.d.ts",
  "devDependencies": {
    "npm-run-all": "^4.1.3",
    "rimraf": "^2.6.2",
    "rollup": "^0.66.2",
    "tslint": "^5.11.0",
    "typescript": "^3.1.1",
    "uglify-es": "^3.3.9"
  },
  "scripts": {
    "_clear": "rimraf build/compiled/*",
    "_tsc": "tsc",
    "_make-bundle": "node build/make-bundle",
    "build": "run-s _clear _tsc _make-bundle",
    "watch": "tsc --watch",
    "lint": "tslint -p tsconfig.json -t verbose",
    "lint:fix": "tslint -p tsconfig.json -t verbose --fix"
  },
  "engines": {
    "node": ">=8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paleo/ladc.git"
  },
  "author": {
    "name": "Paleo"
  },
  "license": "CC0-1.0",
  "keywords": [
    "node.js",
    "sql",
    "connector",
    "relational",
    "database",
    "pool",
    "transaction",
    "jdbc",
    "pdo"
  ]
}
