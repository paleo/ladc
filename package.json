{
  "name": "@ladc/sqlite3-adapter",
  "version": "0.22.0",
  "description": "The LADC adapter to the driver \"sqlite3\" (SQLite).",
  "main": "ladc-sqlite3-adapter.min.js",
  "types": "ladc-sqlite3-adapter.d.ts",
  "dependencies": {
    "@types/sqlite3": "^3.1.5",
    "sqlite3": "^4.1.0"
  },
  "peerDependencies": {
