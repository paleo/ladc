{
  "name": "mycn",
  "version": "0.15.0",
  "description": "A layer above SQL database connectors for Node.js, inspired from PDO and JDBC",
  "main": "mycn.min.js",
  "types": "mycn.d.ts",
  "devDependencies": {
    "npm-run-all": "^4.1.3",
    "rimraf": "^2.6.2",
    "rollup": "^0.66.2",
    "tslint": "^5.11.0",
    "typescript": "^3.1.1",
    "uglify-es": "^3.3.9"
  },
  "scripts": {
    "_clear": "rimraf build/compiled/*",
    "_tsc": "tsc",
    "_make-bundle": "node build/make-bundle",
    "build": "run-s _clear _tsc _make-bundle",
    "watch": "tsc --watch",
    "lint": "tslint -p tsconfig.json -t verbose",
    "lint:fix": "tslint -p tsconfig.json -t verbose --fix"
  },
  "engines": {
    "node": ">=8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paleo/mycn.git"
  },
  "author": {
    "name": "Paleo"
  },
  "license": "CC0-1.0",
  "keywords": [
    "connection",
    "connector",
    "relational",
    "database",
    "pool",
    "transaction",
    "sql",
    "sqlite",
    "postgresql",
    "jdbc",
    "pdo"
  ]
}
