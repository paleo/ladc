{
  "name": "ladc",
  "version": "0.22.0",
  "description": "LADC provides a common API inspired from PDO and JDBC.",
  "main": "ladc.min.js",
  "types": "ladc.d.ts",
  "devDependencies": {
    "npm-run-all": "4",
    "rimraf": "3",
    "rollup": "1",
    "terser": "4",
    "tslint": "5",
    "typescript": "3"
  },
  "scripts": {
    "build": "run-s clear tsc make-bundle",
    "tsc": "tsc",
    "tsc:watch": "tsc --watch",
    "make-bundle": "node build/make-bundle",
    "clear": "rimraf build/compiled/*",
    "lint": "tslint -p tsconfig.json -t verbose",
    "prepublishOnly": "npm run build"
  },
  "engines": {
    "node": ">=8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paleo/ladc.git"
  },
  "author": {
    "name": "Paleo"
  },
  "license": "CC0-1.0",
  "keywords": [
    "node.js",
    "sql",
    "connector",
    "relational",
    "database",
    "pool",
    "transaction",
    "jdbc",
    "pdo"
  ]
}