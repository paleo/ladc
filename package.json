{
  "name": "mycn-sqlite3",
  "version": "0.1.3",
  "description": "The connector for \"mycn\" to the driver \"sqlite3\"",
  "main": "mycn-sqlite3.min.js",
  "types": "mycn-sqlite3.d.ts",
  "dependencies": {
    "mycn": "^0.9.3",
    "sqlite3": "^3.1.13"
  },
  "devDependencies": {
    "rollup": "0.56.5",
    "typescript": "2.7.2",
    "uglify-es": "3.3.9",
    "npm-run-all": "4.1.2",
    "rimraf": "2.6.2"
  },
  "scripts": {
    "_clear": "rimraf tsproject/compiled/*.js tsproject/compiled/*.d.ts",
