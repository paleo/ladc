{
  "name": "ladc",
  "version": "0.22.6",
  "description": "LADC is a common API on top of relational database (SQL) connectors. It connects to Postgresql, MariaDB, MySQL, SQLite.",
  "main": "ladc.min.js",
  "types": "ladc.d.ts",
  "devDependencies": {
    "npm-run-all": "4",
    "rimraf": "3",
    "rollup": "^1.29.0",
    "terser": "^4.6.2",
    "tslint": "^5.20.1",
    "typescript": "^3.7.4"
  },
  "scripts": {
    "build": "run-s clear tsc make-bundle",
    "tsc": "tsc",
    "tsc:watch": "tsc --watch",
    "make-bundle": "node build/make-bundle",
    "clear": "rimraf build/compiled/*",
    "lint": "tslint -p tsconfig.json -t verbose",
    "prepublishOnly": "npm run lint && npm run build"
  },
  "engines": {
    "node": ">=8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paleo/ladc.git"
  },
  "homepage": "https://github.com/paleo/ladc/tree/master/ladc",
  "author": {
    "name": "Paleo"
  },
  "license": "CC0-1.0",
  "keywords": [
    "sqlite",
    "postgresql",
    "mariadb",
    "mysql"
  ]
}
